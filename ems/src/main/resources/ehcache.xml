<?xml version="1.0" encoding="UTF-8"?>
<ehcache updateCheck="false" name="othercache">
    <!--用于ConditionConfig的缓存-->
    <cache name="conditionCache"
           maxElementsInMemory="1000"
           eternal="false"
           timeToIdleSeconds="3600"
           timeToLiveSeconds="0"
           overflowToDisk="false"
           diskPersistent="false"
           memoryStoreEvictionPolicy="LRU"/>

    <!--用于SystemConfig的缓存-->
    <cache name="systemConfigCache"
           maxElementsInMemory="1000"
           eternal="false"
           timeToIdleSeconds="3600"
           timeToLiveSeconds="0"
           overflowToDisk="false"
           diskPersistent="false"
           memoryStoreEvictionPolicy="LRU"/>

</ehcache>