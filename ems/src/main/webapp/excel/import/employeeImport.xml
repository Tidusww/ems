<?xml version="1.0" encoding="utf-8"?>
<workbook>
    <worksheet name="Sheet1">
        <section startRow="0" endRow="0">
        </section>
        <loop startRow="1" endRow="1" items="itemList" var="item"
              varType="com.ly.ems.model.base.employee.Employee">
            <section startRow="1" endRow="1">
                <mapping row="1" col="0">item.employeeName</mapping>
                <mapping row="1" col="1">item.gender</mapping>
                <mapping row="1" col="2">item.idCard</mapping>
                <mapping row="1" col="3">item.location</mapping>
                <mapping row="1" col="4">item.address</mapping>
                <mapping row="1" col="5">item.phone</mapping>
                <mapping row="1" col="6">item.salaryBank</mapping>
                <mapping row="1" col="7">item.salaryAccount</mapping>
                <mapping row="1" col="8">item.socialSecurityNo</mapping>
                <mapping row="1" col="9">item.personalSsAmount</mapping>
                <mapping row="1" col="10">item.companySsAmount</mapping>
                <mapping row="1" col="11">item.houseFundAmount</mapping>
                <mapping row="1" col="12">item.entryDate</mapping>
                <mapping row="1" col="13">item.contractNo</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"></cellcheck>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>